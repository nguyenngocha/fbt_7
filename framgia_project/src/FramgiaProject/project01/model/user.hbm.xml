<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="FramgiaProject.project01.model.User" table="User">  
    <id name="id">  
     <generator class="assigned"></generator>  
    </id>  
            
    <property name="name"></property>  
    <property name="email"></property>  
    <property name="password"></property>
    <property name="phone_number"></property>
    <property name="banking_account"></property>
    <property name="role" type="integer">
      <column name="role" not-null="true" default="0" />
    </property>
            
  </class>  
            
 </hibernate-mapping>  
