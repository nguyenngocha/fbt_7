<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="FramgiaProject.project01.model.Discount" table="Discount">  
    <id name="id">  
     <generator class="assigned"></generator>  
    </id>  
            
    <property name="type" type="integer"></property>
    <property name="value" type="integer"></property>
    <property name="target_id" type="integer"></property>
    <property name="target_type" type="string"></property>
    <property name="start_date" type="date"></property>  
    <property name="end_date" type="date"></property>
    <property name="status" type="integer"></property>
  </class>  
            
 </hibernate-mapping>  
