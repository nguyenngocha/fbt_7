<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="FramgiaProject.project01.model.TourMaster" table="TourMaster">  
    <id name="id">  
     <generator class="assigned"></generator>  
    </id>  
            
    <property name="name"></property>  
    <property name="description"></property>  
    <property name="days" type="integer"></property>
    <property name="price" type="integer"></property>
    <many-to-one name="category_id" class="FramgiaProject.project01.model.Category" column="category_id"
     not-null="true" />
       
  </class>  
            
 </hibernate-mapping>  
